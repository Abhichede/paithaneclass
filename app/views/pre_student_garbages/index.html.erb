<p id="notice"><%= notice %></p>

<h1>Pre Student Garbages</h1>

<table>
  <thead>
    <tr>
      <th>Academic year</th>
      <th>Student</th>
      <th>Fee structure</th>
      <th>Allocated fee</th>
      <th>Paid fee</th>
      <th>Balance fee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pre_student_garbages.each do |pre_student_garbage| %>
      <tr>
        <td><%= pre_student_garbage.academic_year_id %></td>
        <td><%= pre_student_garbage.student_id %></td>
        <td><%= pre_student_garbage.fee_structure_id %></td>
        <td><%= pre_student_garbage.allocated_fee %></td>
        <td><%= pre_student_garbage.paid_fee %></td>
        <td><%= pre_student_garbage.balance_fee %></td>
        <td><%= link_to 'Show', pre_student_garbage %></td>
        <td><%= link_to 'Edit', edit_pre_student_garbage_path(pre_student_garbage) %></td>
        <td><%= link_to 'Destroy', pre_student_garbage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pre Student Garbage', new_pre_student_garbage_path %>
